@model PipelineFeatureList.Models.PTTestMedium

@{
    ViewBag.Title = "Create PT Medium";
}

<h2>Create PT Medium</h2>

@using (Html.BeginForm())
{
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>SpecRating</legend>
        <table>
            <tr>
                <td>
                    <div class="editor-label">
                        @Html.LabelFor(model => model.PTMedium)
                    </div>
                </td>
                <td>
                    <div class="wide">
                        @Html.EditorFor(model => model.PTMedium)
                        @Html.ValidationMessageFor(model => model.PTMedium)
                    </div>
                </td>
            </tr>
        </table>
        <p>
            <input type="submit" value="Create" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index", null, htmlAttributes: new { @class = "linkbutton" })
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
