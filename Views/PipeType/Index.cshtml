@model IEnumerable<PipelineFeatureList.Models.PipeType>

@{
    ViewBag.Title = "Pipe Types";
}

<h2>Pipe Types</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PipeTypeItem)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PipeTypeItem)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PipeTypeID }) |
            @Html.ActionLink("Details", "Details", new { id=item.PipeTypeID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PipeTypeID })
        </td>
    </tr>
}
<div>
    @Html.ActionLink("Back to Admin", "Index", "Admin")
</div>
</table>
