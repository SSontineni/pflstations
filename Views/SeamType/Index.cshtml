@model IEnumerable<PipelineFeatureList.Models.SeamType>

    @{
        ViewBag.Title = "Seam Weld Type";
    }

    <h2>Seam Weld Type</h2>

    <p>
        @Html.ActionLink("Create New", "Create", null, htmlAttributes: new { @class = "linkbutton" })
        @Html.ActionLink("Back to Admin", "Index", "Admin", null, htmlAttributes: new { @class = "linkbutton" })
    </p>
    <table>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SeamTypeItem)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JointFactor)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.SeamTypeItem)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JointFactor)
            </td>
            <td>

                @Html.ActionLink("Edit", "Edit", new { id = item.SeamTypeID }, htmlAttributes: new { @class = "linkbutton" })
                @Html.ActionLink("Details", "Details", new { id = item.SeamTypeID }, htmlAttributes: new { @class = "linkbutton" })
                @Html.ActionLink("Delete", "Delete", new { id = item.SeamTypeID }, htmlAttributes: new { @class = "linkbutton" })
            </td>
        </tr>
        }
    </table>
    <div>
        @Html.ActionLink("Back to Admin", "Index", "Admin", null, htmlAttributes: new { @class = "linkbutton" })
    </div>
