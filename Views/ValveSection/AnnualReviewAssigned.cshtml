@model IEnumerable<PipelineFeatureList.Models.ValveSectionAnnualReview>

@{
    ViewBag.Title = "AnnualReview";
}

<table width="100%">
    <tr>
        <th>
            @Html.Raw("Station Location")
        </th>
        <th>
            @Html.Raw("Station")
        </th>
        <th>
            @Html.Raw("Circuit")
        </th>
        <th>
            @Html.Raw("Status")
        </th>
    </tr>

    
@foreach (var item in Model)
{
    if (item.ValveSectionAnnualReviewAssigned != null)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ValveSectionAnnualReviewAssigned.PipeSystem.PipeSystemItem)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValveSectionAnnualReviewAssigned.Pipeline.PipelineItem)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValveSectionAnnualReviewAssigned.ValveSection.ValveSectionBegin)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValveSectionAnnualReviewAssigned.ValveSection.OrionStationSeries)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValveSectionAnnualReviewAssigned.ValveStatus.ValveSectionStatusItem)
            </td>
            <td>
                @Html.ActionLink("Overview", "", "Overview", new { ValveSectionID = item.ValveSectionAnnualReviewAssigned.ValveSection.ValveSectionID, OrionStationSeries = item.ValveSectionAnnualReviewAssigned.ValveSection.OrionStationSeries }, htmlAttributes: new { @class = "linkbutton" })
            </td>
        </tr>
    }
}
</table>
